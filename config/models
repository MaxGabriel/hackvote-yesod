HackDay
    title Text
    created UTCTime default=CURRENT_TIMESTAMP
    votingClosed Bool default=FALSE
    deriving Show
Project
	hackday HackDayId
	title Text
	creators Text
	votes Int default=0
	created UTCTime default=CURRENT_TIMESTAMP
	deriving Show

